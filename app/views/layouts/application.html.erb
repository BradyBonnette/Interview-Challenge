<!DOCTYPE html>
<html>
	<head>
		<title>Interview</title>
		<%= stylesheet_link_tag :all %>
		<%= javascript_include_tag :defaults %>
		<%= csrf_meta_tag %>
	</head>
	<body>

		<% if flash[:notice] %>
			<div id="notice">
				<img src="images/error.png" alt="image">
				<p>There was an error processing your request:  <%= flash[:notice] %></p>
			</div>
		<% end %>

		<div id="container">
			<%= yield %>
		</div>
	</body>
</html>
